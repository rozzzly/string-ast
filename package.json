{
  "name": "string-ast",
  "version": "0.0.1",
  "description": "Having trouble manipulating strings with ANSI styles (eg: chalk) or certain Unicode characters? Hammer that nail with an anvil; make an AST out of it!",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "test:watch": "ava -w",
    "dev": "npm-run-all -s build -p build:watch test:watch",
    "build": "tsc",
    "build:watch": "tsc -w",
    "build:es6": "tsc -p ./tsconfig.es2015.json",
    "build:legacy": "tsc -p ./tsconfig.legacy.json"
  },
  "repository": "rozzzly/string-ast",
  "keywords": [
    "ansi",
    "unicode",
    "chalk",
    "console",
    "terminal",
    "cli",
    "string",
    "multi-byte",
    "emoji",
    "ast",
    "format"
  ],
  "author": "rozzzly",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.13",
    "purdy": "^3.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.105",
    "@types/node": "^9.4.7",
    "ava": "^0.25.0",
    "npm-run-all": "^4.1.2",
    "tslint": "^5.9.1",
    "typelevel-ts": "^0.2.3",
    "typescript": "^2.8.0-dev.20180320",
    "typical-ts": "^0.2.0"
  },
  "ava": {
    "files": [
      "./bin/test/**/*.js"
    ]
  }
}
